<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.deezy.quizredux">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudyActivity"
            android:screenOrientation="portrait"
            android:label="@string/study_header"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".TestActivity"
            android:screenOrientation="portrait"
            android:label="@string/test_header"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".HighScoreActivity"
            android:screenOrientation="portrait"
            android:label="@string/high_score_header"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".FeedbackActivity"
            android:screenOrientation="portrait"
            android:label="@string/feedback_header"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".HiriganaActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".KatakanaActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".KanaguideActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionOneActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionTwoActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionThreeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionFourActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionFiveActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionSixActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionSevenActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionEightActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionNineActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuestionTenActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".TestEndActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>